{
  "expo": {
    "name": "50 dias",
    "slug": "project-50-tracker",
    "scheme": "project50tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icono.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F9F7F2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alvaro16.project50tracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icono.png",
        "backgroundColor": "#F9F7F2"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#F9F7F2"
      },
      "notification": {
        "icon": "./assets/icono-notification.png",
        "color": "#FF231F7C"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.alvaro16.project50tracker",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icono-notification.png",
          "color": "#FF231F7C",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "64186c9d-596a-425d-b453-3a6a24b022be"
      }
    }
  }
}
