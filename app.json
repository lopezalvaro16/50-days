      "predictiveBackGestureEnabled": false,
      "package": "com.alvaro16.project50tracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "64186c9d-596a-425d-b453-3a6a24b022be"
      }
    }
  }
}
